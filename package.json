{
  "name": "plataforma academica y avatar con live 2D ",
  "description": "generacion de contenedores del frontend que consumiran los cursos e interaccion con un avatar",

  "scripts": {
    "start": "cd Samples/TypeScript/Demo && npm run-script serve && cd cursos && ng serve" ,
    "prestart": "cd Samples/TypeScript/Demo && npm run-script build",
    "poststart": "cd cursos && ng servex" ,
    "build": "cd Samples/TypeScript/Demo && npm run-script build" ,
    "build:prod": "webpack --mode production",
    "test": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "serve": "serve ../../.. -p 5000",
    "servex": "serve ../../.. -p 4200",
    "clean": "rimraf dist"
  },
